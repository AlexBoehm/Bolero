<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RunCommand>dotnet</RunCommand>
    <RunArguments>blazor serve</RunArguments>
    <DefineConstants Condition="'$(GhPages)' != ''">$(DefineConstants);GHPAGES</DefineConstants>
    <MiniBlazorTaskAssemblyPath>$(MSBuildThisFileDirectory)..\..\src\MiniBlazor.Build\bin\$(Configuration)\netstandard2.0\MiniBlazor.Build.dll</MiniBlazorTaskAssemblyPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Main.fs" />
    <Compile Include="Startup.fs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\src\MiniBlazor\MiniBlazor.fsproj" />
  </ItemGroup>
  <Import Project="..\..\src\MiniBlazor.Build\MiniBlazor.Build.targets" />
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>
